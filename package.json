{
	"name": "useful-unknown-website-comments",
	"scripts": {
		"postinstall": "prettier-config-generate",
		"lint": "eslint . --ignore-path .gitignore --ext .ts --color",
		"format": "prettier .",
		"format-write": "pnpm format --write",
		"format-check": "pnpm format --check",
		"typecheck": "tsc --noEmit",
